#include <iostream>  
using namespace std;  
int main()  
{  
  int n,digit,sum=0,temp;    
  cout<<"Enter the Number=";    
  cin>>n;    
 temp=n;    
 while(n>0)    
{    
 digit=n%10;    
 sum=(sum*10)+digit;    
 n=n/10;    
}    
if(temp==sum)    
cout<<"Palindrome.";    
else    
cout<<"not Palindrome.";   
  return 0;  
}
